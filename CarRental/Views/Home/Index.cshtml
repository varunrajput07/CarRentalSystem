<div class="hero" style="background-image: url('images/hero_1_a.jpg');">

        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-lg-10">

                    <div class="row mb-5">
                        <div class="col-lg-7 intro">
                            <h1><strong>Rent a car</strong> is within your finger tips.</h1>
                        </div>
                    </div>

                    <form class="trip-form" method="get" asp-area="" asp-action="SearchCar">
                        <div class="row align-items-center">

                            <div class="mb-2 mb-md-0 col-md-2">
                                <div class="form-control-wrap">
                                    <input type="text" placeholder="Searching..." value=" @ViewData["GetCardetails"]" name="filter" class="form-control px-2" />
                                </div>

                            </div>
                            <div class="mb-2 mb-md-0 col-md-2">
                                <div class="form-control-wrap">
                                    <input type="text" id="" placeholder="No.Of Seates" name="numOfSeats" class="form-control px-2">

                                </div>
                            </div>


                            <div class="mb-2 mb-md-0 col-md-2">
                                <select name="" id="" asp-items="ViewBag.CarModalList" class="custom-select form-control">
                                    <option disabled selected>--Select CarModal--</option>

                                </select>
                            </div>

                            <div class="mb-2 mb-md-0 col-md-2">
                                <div class="form-control-wrap">
                                    <input type="text" id="cf-3" placeholder="Pick up" name="BookingStartDate" class="form-control datepicker px-2 " required/>
                                    <span class="icon icon-date_range"></span>

                                </div>
                            </div>
                            <div class="mb-2 mb-md-0 col-md-2">
                                <div class="form-control-wrap">
                                    <input type="text" id="cf-4" placeholder="Drop off" name="BookingEndDate" class="form-control datepicker px-2" required/>
                                    <span class="icon icon-date_range"></span>
                                </div>
                            </div>

                            <div class="mb-2 mb-md-0 col-md-2">
                                <div class="form-control-wrap">

                                    <input type="submit" value="Find Car" class="btn btn-primary btn-block py-3" @*onchange=("comparedates")*@ />
                                </div>

                            </div>
                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
